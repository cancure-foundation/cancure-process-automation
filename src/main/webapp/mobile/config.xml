<?xml version='1.0' encoding='utf-8'?>
<widget id="site.cancure" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cancure</name>
    <description>
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="StatusBarOverlaysWebView" value="true" />
        <preference name="android-minSdkVersion" value="19" />
        <icon density="ldpi" src="res/icon/android/ldpi/ic_launcher.png" />
        <icon density="mdpi" src="res/icon/android/mdpi/ic_launcher.png" />
        <icon density="hdpi" src="res/icon/android/hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="res/icon/android/xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="res/icon/android/xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="res/icon/android/xxxhdpi/ic_launcher.png" />
        <splash density="port-hdpi" src="res/screen/android/hdpi@480x800.png" />
        <splash density="port-ldpi" src="res/screen/android/ldpi@200x320.png" />
        <splash density="port-mdpi" src="res/screen/android/mdpi@320x480.png" />
        <splash density="port-xhdpi" src="res/screen/android/xhdpi@720px1280.png" />
        <splash density="port-xxhdpi" src="res/screen/android/xxhdpi@960x1600.png" />
        <splash density="port-xxxhdpi" src="res/screen/android/xxxhdpi@1280x1920.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="deployment-target" value="10.0" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <icon height="180" src="res/icon/ios/Icon-App-60x60@3x.png" width="180" />
        <icon height="60" src="res/icon/ios/Icon-App-60x60@1x.png" width="60" />
        <icon height="120" src="res/icon/ios/Icon-App-60x60@2x.png" width="120" />
        <icon height="76" src="res/icon/ios/Icon-App-76x76@1x.png" width="76" />
        <icon height="152" src="res/icon/ios/Icon-App-76x76@2x.png" width="152" />
        <icon height="40" src="res/icon/ios/Icon-App-40x40@1x.png" width="40" />
        <icon height="80" src="res/icon/ios/Icon-App-40x40@2x.png" width="80" />
        <icon height="57" src="res/icon/ios/Icon-App-57x57@1x.png" width="57" />
        <icon height="114" src="res/icon/ios/Icon-App-57x57@2x.png" width="114" />
        <icon height="72" src="res/icon/ios/Icon-App-72x72@1x.png" width="72" />
        <icon height="144" src="res/icon/ios/Icon-App-72x72@2x.png" width="144" />
        <icon height="167" src="res/icon/ios/Icon-App-83.5x83.5@2x.png" width="167" />
        <icon height="29" src="res/icon/ios/Icon-App-29x29@1x.png" width="29" />
        <icon height="58" src="res/icon/ios/Icon-App-29x29@2x.png" width="58" />
        <icon height="50" src="res/icon/ios/Icon-Small-50x50@1x.png" width="50" />
        <icon height="100" src="res/icon/ios/Icon-Small-50x50@2x.png" width="100" />
        <icon height="167" src="res/icon/ios/Icon-App-83.5x83.5@2x.png" width="167" />
        <splash src="res/screen/ios/Default@2x~iphone~anyany.png" />
        <splash src="res/screen/ios/Default@2x~iphone~comany.png" />
        <splash src="res/screen/ios/Default@2x~iphone~comcom.png" />
        <splash src="res/screen/ios/Default@3x~iphone~anyany.png" />
        <splash src="res/screen/ios/Default@3x~iphone~anycom.png" />
        <splash src="res/screen/ios/Default@3x~iphone~comany.png" />
        <splash src="res/screen/ios/Default@2x~ipad~anyany.png" />
        <splash src="res/screen/ios/Default@2x~ipad~comany.png" />
        <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
            <string>Bundyon need camera access to take pictures</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
            <string>Bundyon need to photo library access to get pictures from there</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
            <string>Bundyon need location access to find things nearby</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryAddUsageDescription">
            <string>Bundyon need to photo library access to save pictures there</string>
        </edit-config>
    </platform>
    <preference name="UIWebViewBounce" value="false" />
    <preference name="UIWebViewDecelerationSpeed" value="normal" />
    <preference name="StatusBarBackgroundColor" value="#dc7c00" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="orientation" value="portrait" />
    <preference name="SplashScreenDelay" value="4500" />
    <preference name="FadeSplashScreenDuration" value="500" />
    <plugin name="cordova-plugin-speechrecognition" spec="^1.1.2" />
    <plugin name="cordova-plugin-camera" spec="^3.0.0" />
    <plugin name="cordova-plugin-device" spec="^1.1.7" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.4" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.1.0" />
    <plugin name="cordova-plugin-statusbar" spec="^2.3.0" />
    <plugin name="onesignal-cordova-plugin" spec="^2.2.5" />
    <engine name="android" spec="^7.0.0" />
</widget>
